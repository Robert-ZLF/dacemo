###  https:/www./docemo.org/owl/examples/iteration-0/Exposure
:Exposure rdf:type owl:Class ;
          rdfs:subClassOf dcm:Event ;
          dcm:description "Any event of exposing magic to Muggles" ;
          dcm:isTopConcept "true"^^xsd:boolean .


###  https:/www./docemo.org/owl/examples/iteration-0/MagicalExposures
:MagicalExposures rdf:type owl:Class ;
                  rdfs:subClassOf dcm:BusinessMeasure ;
                  dcm:description "Measure of exposures of magical activities, either in number, in impact or in some other way" .


###  https:/www./docemo.org/owl/examples/iteration-0/MoM
:MoM rdf:type owl:Class ;
     rdfs:subClassOf dcm:Entity ;
     dcm:description "Fictional Ministry of Magic from the JK Rowling Harry Potter series was used as syntetic example in order ot avoid conflicts with real-life Ministries." ;
     dcm:isTopConcept "true"^^xsd:boolean ;
     rdfs:label "Ministry of Magic" .


###  https:/www./docemo.org/owl/examples/iteration-0/Muggle
:Muggle rdf:type owl:Class ;
        rdfs:subClassOf dcm:Person ;
        dcm:description "Non-magical person." ;
        dcm:isTopConcept "true"^^xsd:boolean .


